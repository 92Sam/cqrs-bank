
enum UserStatus {
    ENABLED
    DISABLED
}

type User {
    id: ID
    name: String
    email:String
    userStatus: UserStatus
    password: String
    createDate: String
    updatedAt: String
}

input UserInput {
    email: String
    name: String
    password: String
}

input UserAccountInput {
    email: String
    name: String
    password: String
    initialDeposit: InputInitialDepositAccount!
}

input InputInitialDepositAccount {
    currency: Currency!
    amount: Float!
}